{
  "server": {
    "port": 8000,
    "apiVersion": "v1",
    "nodeEnv": "development",
    "host": "localhost"
  },
  "rateLimit": {
    "windowMs": 900000,
    "maxRequests": 100,
    "authWindowMs": 60000,
    "authMaxRequests": 5
  },
  "fileUpload": {
    "maxFileSize": 5242880,
    "uploadPath": "uploads/",
    "allowedTypes": ["image/jpeg", "image/png", "image/gif", "application/pdf"]
  },
  "appointment": {
    "defaultDuration": 30,
    "workingHours": {
      "start": "09:00",
      "end": "17:00"
    },
    "reminderTimes": [24, 1]
  }
}
